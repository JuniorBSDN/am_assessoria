{
  "version": 2,
  "builds": [
    {
      "src": "api/cadastro.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/cadastro",
      "methods": ["POST"],
      "dest": "api/cadastro.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
